
public class BasePlusCommissionEmployeeTest {

	@Test
	public void test() {
		BasePlusCommissionEmployee test1 = 
				new BasePlusCommissionEmployee(29, "Brandon", "Koo", 0, 30, 0.2);
		
		BasePlusCommissionEmployee test2 = 
				new BasePlusCommissionEmployee(60, "John", "Smith", 400, 30, 0.7);
		
		test1.toString();
		double result1 = test1.DoubleEarning();
		
		test2.toString();
		double result2 = test2.DoubleEarning();
		
		// print test1
		System.out.printf(test1.toString());
		System.out.printf("%0.2f",result1);
		
		// print test2
		System.out.printf(test1.toString());
		System.out.printf("%0.2f",result2);
	}

}
